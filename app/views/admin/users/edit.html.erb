<%= form_for :user, :url => 'admin_users_update_user_path' do |f|%>
<div class= 'field'>
<%= f.label :user_name%>
<%= f.text_field :user_name%>
</div>
<div class= 'field'>
<%= f.label :first_name%>
<%= f.text_field :first_name%>
</div>
<div class= 'field'>
<%= f.label :last_name%>
<%= f.text_field :last_name%>
</div>
<div class= 'field'>
<%= f.label :email%>
<%= f.email_field :email%>
</div>
<%= f.submit 'edit'%>

<%end%>